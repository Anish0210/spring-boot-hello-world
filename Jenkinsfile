pipeline {
  agent any
  stages {
    stage('Checkout') {
            steps {
               
                    git credentialsId: 'GITHUB', url: 'https://github.com/Anish0210/spring-boot-hello-world'
                
            }
        }
      
    stage ('Build') {
      steps {
        echo 'Running Build Automation '
        
            }
          }
        }
      }
